<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @category do |f| %>
      <div class="form-group">
        <%= f.text_field :name, class: "form-control", placeholder: "Category" %>
      </div>
      <div class="btn_x">
        <%= f.submit "Submit", class: "btn btn-default btn-x" %>
      </div>
    <% end %>
  </div>
</div>
