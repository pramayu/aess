<%= title "Event Category" %>
<div class="event container">
  <div class="data-event">
    <% @categories.each do |category| %>
      <div class="col-md-3 event_">
        <h4 class="_ev">
          <%= link_to "#{category.name}", category_event_url(category), class: "name_", "data-text": "#{category.name}", "data-turbolinks": false %>
        </h4>
      </div>
    <% end %>
  </div>
</div>
<div class="header_texture _texture">
  <h1 class="o-title-flex -texture">
    <span class="c-char">G</span>
    <span class="c-char">A</span>
    <span class="c-char">L</span>
    <span class="c-char">E</span>
    <span class="c-char">R</span>
    <span class="c-char">I</span>
  </h1>
</div>
